        "padding": {
          "unit": "px",
          "top": "0",
          "right": "40",
          "bottom": "80",
          "left": "40"
        },
        "background_color": "#FFFFFF"
      },
      "elements": [
        {
          "id": "type-1-col",
          "elType": "column",
          "settings": {
            "_column_size": 33,
            "background_color": "#F8F9FA",
            "padding": {
              "unit": "px",
              "top": "40",
              "right": "30",
              "bottom": "40",
              "left": "30"
            },
            "border_radius": {
              "unit": "px",
              "size": 8
            }
          },
          "elements": [
            {
              "id": "type-1-icon",
              "elType": "widget",
              "widgetType": "icon",
              "settings": {
                "icon": "fas fa-code-branch",
                "primary_color": "#3E92CC",
                "size": {
                  "unit": "px",
                  "size": 50
                },
                "align": "center"
              }
            },
            {
              "id": "type-1-title",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Integration Partnership",
                "header_size": "h3",
                "align": "center",
                "title_color": "#0A2463",
                "typography_font_size": {
                  "unit": "px",
                  "size": 24
                }
              }
            },
            {
              "id": "type-1-text",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<p style='text-align: center; color: #5A6C7D; line-height: 1.7;'>Seamlessly integrate Dataphiles patient communication capabilities into your PMS platform. Full API access with white-label options.</p>"
              }
            }
          ]
        },
        {
          "id": "type-2-col",
          "elType": "column",
          "settings": {
            "_column_size": 33,
            "background_color": "#F8F9FA",
