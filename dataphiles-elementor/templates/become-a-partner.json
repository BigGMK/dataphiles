          "elements": [
            {
              "id": "partner-hero-title",
              "elType": "widget",
              "widgetType": "heading",
              "settings": {
                "title": "Let's Build the Future of Patient Engagement Together",
                "header_size": "h1",
                "align": "center",
                "title_color": "#FFFFFF",
                "typography_font_size": {
